<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>RegistrationServlet</servlet-name>
  	<display-name>RegistrationServlet</display-name>
  	<description></description>
  	<servlet-class>com.cdac.simplelogin.RegistrationServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LoginServlet</servlet-name>
  	<display-name>LoginServlet</display-name>
  	<description></description>
  	<servlet-class>com.cdac.cookie.LoginServlet</servlet-class>
  </servlet>
  
  <servlet>
  	<servlet-name>Inbox</servlet-name>
  	<display-name>Inbox</display-name>
  	<description></description>
  	<servlet-class>com.cdac.cookie.Inbox</servlet-class>
  </servlet>
  
  <servlet>
  	<servlet-name>SendItems</servlet-name>
  	<display-name>SendItems</display-name>
  	<description></description>
  	<servlet-class>com.cdac.cookie.SendItems</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LifeCycle</servlet-name>
  	<display-name>LifeCycle</display-name>
  	<description></description>
  	<servlet-class>com.cdac.lifecycle.LifeCycle</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>MainApplication</servlet-name>
  	<display-name>MainApplication</display-name>
  	<description></description>
  	<servlet-class>com.cdac.forwarding.MainApplication</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Servlet1</servlet-name>
  	<display-name>Servlet1</display-name>
  	<description></description>
  	<servlet-class>com.cdac.forwarding.Servlet1</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Servlet2</servlet-name>
  	<display-name>Servlet2</display-name>
  	<description></description>
  	<servlet-class>com.cdac.forwarding.Servlet2</servlet-class>
  </servlet>
 
  <servlet-mapping>
  	<servlet-name>RegistrationServlet</servlet-name>
  	<url-pattern>/RegistrationServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LoginServlet</servlet-name>
  	<url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>Inbox</servlet-name>
  	<url-pattern>/Inbox</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>SendItems</servlet-name>
  	<url-pattern>/SendItems</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LifeCycle</servlet-name>
  	<url-pattern>/LifeCycle</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>MainApplication</servlet-name>
  	<url-pattern>/MainApplication</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Servlet1</servlet-name>
  	<url-pattern>/Servlet1</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Servlet2</servlet-name>
  	<url-pattern>/Servlet2</url-pattern>
  </servlet-mapping>
</web-app>
